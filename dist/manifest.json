{
    "manifest_version": 3,
    "name": "Scrapper",
    "version": "1.0.0",
    "background": {
        "service_worker": "sw.js"
    },
    "action": {
        "default_title": "Click Me",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "https://www.linkedin.com/*"
    ],
    "externally_connectable": {
        "matches": [
            "https://www.linkedin.com/*"
        ]
    }
}